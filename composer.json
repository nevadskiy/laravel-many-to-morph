{
    "name": "nevadskiy/laravel-many-to-morph",
    "description": "The missing Eloquent polymorphic relationship.",
    "keywords": [
        "many-to-morph",
        "many-to-any",
        "relation",
        "laravel",
        "eloquent"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0.2",
        "illuminate/database": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Nevadskiy\\ManyToMorph\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nevadskiy\\ManyToMorph\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

{
    "name": "nevadskiy/laravel-many-to-morph",
    "description": "The missing Eloquent polymorphic relationship.",
    "keywords": [
        "many-to-morph",
        "many-to-any",
        "relation",
        "laravel",
        "eloquent"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0.2",
        "illuminate/database": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.37|^7.34|^8.14"
    },
    "autoload": {
        "psr-4": {
            "Nevadskiy\\ManyToMorph\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nevadskiy\\ManyToMorph\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
